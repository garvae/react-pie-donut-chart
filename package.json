{
  "version": "1.0.0",
  "description": "Lightweight library allows you to create \"pie\" and \"donut\" charts easily",
  "keywords": [
    "arc",
    "arc-chart",
    "area-chart",
    "chart",
    "charting",
    "charts",
    "component",
    "data visualization",
    "donut-chart",
    "pie",
    "pie-chart",
    "radial",
    "react",
    "react-component",
    "reactjs",
    "svg",
    "typescript",
    "visualization"
  ],
  "homepage": "https://github.com/garvae/react-pie-donut-chart",
  "funding": {
    "url": "https://ko-fi.com/garvae"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garvae/react-pie-donut-chart"
  },
  "bugs": {
    "url": "https://github.com/garvae/react-pie-donut-chart/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/index.js",
  "module": "dist/react-pie-donut-chart.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "size-limit",
    "build": "yarn rollup:compile",
    "build:index-cleanup": "node scripts/clean-dist-index.js",
    "copy:license": "cpx LICENSE dist",
    "copy:readme": "cpx README.md dist",
    "copy:types": "cpx src/types/index.d.ts dist",
    "delete:dist": "rimraf dist",
    "delete:sourcemaps": "del dist/**/*.js.map",
    "generate:packagejson": "node scripts/generate-package-json.js",
    "generate:ts-config": "node scripts/generate-ts-config.js && prettier --write tsconfig.json",
    "lint:js": "cross-env LINT=true eslint src/**/*.{ts,tsx} --fix",
    "postbuild": "yarn build:index-cleanup && yarn generate:packagejson && yarn copy:license && yarn copy:types && yarn copy:readme",
    "postinstall": "husky install",
    "postprepare": "yarn generate:packagejson",
    "pre-commit": "yarn lint:js && yarn test",
    "prebuild": "yarn generate:ts-config && yarn delete:dist && yarn lint:js",
    "prepare": "husky install",
    "pretest": "yarn generate:ts-config",
    "publish:dist": "cd dist && npm publish",
    "rollup:compile": "rollup src/index.ts --file dist/index.js --format cjs -c rollup.config.js",
    "size": "size-limit",
    "test": "jest",
    "test:cover": "jest --collect-coverage",
    "version:patch": "npm version patch"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "name": "@garvae/react-pie-donut-chart",
  "author": "Garvae",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/generator": "^7.18.12",
    "@babel/parser": "^7.18.11",
