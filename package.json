    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^28.1.3",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-postcss": "^0.3.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
